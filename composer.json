{
    "name": "winzou/state-machine-bundle",
    "type": "symfony-bundle",
    "description": "Bundle for the very lightweight yet powerful PHP state machine",
    "keywords": ["symfony", "bundle", "statemachine"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Bacco",
            "homepage": "http://alex.bacco.fr"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
              "name": "winzou/state-machine",
              "version": "0.2",
              "dist": {
                "url": "https://github.com/IgniteYourProject/state-machine/archive/master.zip",
                "type": "zip"
              },
              "source": {
                "url": "https://github.com/IgniteYourProject/state-machine.git",
                "type": "git",
                "reference": "0.2"
              },
              "autoload": {
                "psr-0": { "SM": "src/" }
              }
            }
        }
    ],
    "require": {
        "php":                      ">5.3.0",
        "symfony/framework-bundle": "~2.1"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "psr-4": { "winzou\\Bundle\\StateMachineBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "spec\\": "spec/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
